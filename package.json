{
  "name": "nodejs-express-mongodb",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.Js, Express and MongoDB",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn ./src/server.ts --trace-warnings",
    "test": "set NODE_ENV=test&&jest --silent",
    "test:routes": "set NODE_ENV=test&&jest -i tests/routes.test.js -t",
    "test:journals": "set NODE_ENV=test&&jest -i tests/journal.test.js -t",
    "test:watch": "set NODE_ENV=test&&jest --watchAll",
    "build": "tsc",
    "start:production": "set NODE_ENV=production&&node ./dist/server.js",
    "start:dev": "set NODE_ENV=dev&&nodemon server.js",
    "start:test": "set NODE_ENV=test&&nodemon server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "afletcher",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/hapi__joi": "^17.1.6",
    "@types/mongoose": "^5.11.97",
    "auth0": "^2.35.0",
    "axios": "^0.21.1",
    "axios-request-throttle": "^1.0.0",
    "bull": "^3.22.7",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.5",
    "joi-objectid": "^3.0.1",
    "json-api-serializer": "^2.6.2",
    "jsonapi-serializer": "^3.6.7",
    "jwks-rsa": "^2.0.3",
    "mongoose": "^5.12.11",
    "nodemon": "^2.0.7",
    "redis": "^3.1.2",
    "ts-node-dev": "^1.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bull": "^3.15.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/redis": "^2.8.29",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^26.6.3",
    "supertest": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "__tests__/functions"
    ]
  }
}
