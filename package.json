{
  "name": "nodejs-express-mongodb",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.Js, Express and MongoDB",
  "main": "server.js",
  "scripts": {
    "test": "set NODE_ENV=test&&jest",
    "test:routes": "set NODE_ENV=test&&jest -i tests/routes.test.js -t",
    "test:watch": "set NODE_ENV=test&&jest --watchAll",
    "start:production": "set NODE_ENV=production&&nodemon server.js",
    "start:test": "set NODE_ENV=test&&nodemon server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongodb",
    "rest",
    "api"
  ],
  "author": "afletcher",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "auth0": "^2.35.0",
    "axios": "^0.21.1",
    "axios-request-throttle": "^1.0.0",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.6.0",
    "joi-objectid": "^3.0.1",
    "json-api-serializer": "^2.6.2",
    "jsonapi-serializer": "^3.6.7",
    "jwks-rsa": "^2.0.3",
    "mongoose": "^5.12.11",
    "nodemon": "^2.0.7",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.23.3",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
